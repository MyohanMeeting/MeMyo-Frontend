{
  "testEnvironment": "jsdom",
  "roots": ["<rootDir>/"],
  "moduleNameMapper": {
      "\\.(png|pdf|svg|jpg|jpeg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|styl|less|sass|scss|svg)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": ["<rootDir>/jest-setup.ts"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testMatch": ["<rootDir>/**/*.test.(js|jsx|ts|tsx)"]
}